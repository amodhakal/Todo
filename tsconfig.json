  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "node",
    "rootDir": "src/server",
    "outDir": "dist/server",
    "esModuleInterop": true,
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true,
  },
  "exclude": ["src/client"]
}