{
  "name": "todo",
  "license": "MIT",
  "type": "module",
  "main": "src/server/index.ts",
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "yarn && yarn prisma:generate && yarn dev:ts && cd src/client && yarn && yarn build && cd ../../ && yarn start",
    "prisma:generate": "npx prisma migrate dev --name init",
    "dev:ts": "tsc",
    "dev:ts:run": "yarn dev:ts && yarn start",
    "dev:tsw": "tsc -w",
    "dev:tsw:run": "concurrently \"yarn dev:tsw\" \"nodemon dist/server/index.js\""
  },
  "dependencies": {
    "@prisma/client": "4.10.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.10.1",
    "typescript": "^4.9.5"
  }
}
